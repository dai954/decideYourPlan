<div class="content">
  <div class = "content-image" id= "content-image__left">
    <%= image_tag "#{@subplace.image}", size: '400x250' %>
  </div>
  <div class = "content-text">
    <h1 class = "content-text__time"> <%= display_subplace_arrival() %> </h1>
    <p class = "content-text__name"> <%= "#{@subplace.name}" %> </p>
    <p>タイトル</p>
    <p> <%= "#{@subplace.description}" %> </p>
  </div>
</div>